   <children>
      <Label text="GOALS:" />
      <Separator prefWidth="200.0" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Bedtime: " />
            <TextField fx:id="bedTimeText" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="wakeup time:" />
            <TextField fx:id="wakeUpText" />
         </children>
      </HBox>
      <Label text="Target ammount of sleep:" />
   </children>
</VBox>
