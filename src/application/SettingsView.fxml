<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SettingsController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#switchMainMenuView" text="Back">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <Label text="Settings">
               <HBox.margin>
                  <Insets left="25.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
      <Button alignment="TOP_LEFT" mnemonicParsing="false" onAction="#switchGoalSettingView" prefHeight="25.0" prefWidth="179.0" text="Goals  ">
         <VBox.margin>
            <Insets left="10.0" top="10.0" />
         </VBox.margin></Button>
      <Button alignment="TOP_LEFT" mnemonicParsing="false" onAction="#switchPersonalInformationView" prefHeight="25.0" prefWidth="180.0" text="Personal Information">
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin></Button>
   </children>
</VBox>
